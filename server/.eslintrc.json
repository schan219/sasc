{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:security/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "security"
    ],
    "rules": {
        "indent": [1, 4],
        "semi": 1,
        "new-cap": [2, {"capIsNewExceptions": ["express.Router"]}]
    }
}